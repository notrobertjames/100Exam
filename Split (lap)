def lap(self):
        h, m, s = map(int, self.d.split(":"))
        h = str(h).zfill(2)
        m = str(m).zfill(2)
        s = str(s).zfill(2)
        lp = (f"{h}:{m}:{s}")
        self.lbl = StringVar()
        self.lbl.set(f"{lp}")
        self.lapbox.insert(END, (f"{h}:{m}:{s}"))
        self.lapbox.yview_moveto(1)
